{
  "name": "bachelor-application-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3001",
    "prebuild": "prettier --write \"src/**/*.{ts,tsx}\" && next lint --fix",
    "build": "next build",
    "build:standalone": "next build --turbopack && cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/",
    "start": "next start -p 3001",
    "start:standalone": "PORT=3001 node .next/standalone/server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:cleanup": "tsx src/prisma/cleanup.ts && docker compose down -v && rm -rf ./azurite/* && docker compose up -d && sleep 2 && pnpm db:migrate && pnpm db:seed",
    "buggybastard": "rm -rf node_modules .next && pnpm install && pnpm db:migrate && pnpm db:generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:clear-cache": "jest --clearCache",
    "test:coverage": "jest --coverage"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "tsx src/prisma/seed.ts"
  },
